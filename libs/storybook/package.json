{
  "name": "@design-system/storybook",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {},
  "nx": {
    "targets": {
      "build-storybook": {
        "executor": "@nx/storybook:build",
        "outputs": [
          "{options.outputDir}"
        ],
        "options": {
          "outputDir": "dist/storybook",
          "configDir": "libs/storybook/.storybook"
        },
        "configurations": {
          "ci": {
            "quiet": true
          }
        }
      }
    }
  }
}
