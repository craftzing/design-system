{
  "name": "@design-system/tokens",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "sourceRoot": "libs/tokens",
    "targets": {
      "build-tokens": {
        "executor": "@design-system/build-tokens:build-tokens",
        "options": {
          "source": "path-to-tokens.json",
          "outputSets": "path-to-tokens/sets",
          "outputTokens": "path-to-tokens/build"
        }
      }
    }
  },
  "dependencies": {}
}
